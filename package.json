		"discord.js": "^14.11.0"
	},
	"devDependencies": {
		"@types/node": "^20.3.1",
		"dotenv": "^16.3.1",
		"dotenv-cli": "^7.2.1",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3",
		"unbuild": "^1.2.1"
	}
